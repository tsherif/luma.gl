et":2182},"end":{"line":53,"column":316,"offset":2473}}}],"position":{"start":{"line":53,"column":1,"offset":2158},"end":{"line":53,"column":316,"offset":2473}}},{"type":"text","value":"\n"},{"type":"element","tagName":"li","properties":{},"children":[{"type":"element","tagName":"strong","properties":{},"children":[{"type":"text","value":"Expose WebGL2 to Programmers","position":{"start":{"line":54,"column":5,"offset":2478},"end":{"line":54,"column":33,"offset":2506}}}],"position":{"start":{"line":54,"column":3,"offset":2476},"end":{"line":54,"column":35,"offset":2508}}},{"type":"text","value":" - while many WebGL frameworks make efforts to hide and wrap the WebGL2 API, luma.gl intentionally exposes it, providing JavaScript classes corresponding to all WebGL objects defined in the ","position":{"start":{"line":54,"column":35,"offset":2508},"end":{"line":54,"column":225,"offset":2698}}},{"type":"element","tagName":"a","properties":{"href":"https://www.khronos.org/registry/webgl/specs/latest/2.0/"},"children":[{"type":"text","value":"WebGL2 Specification","position":{"start":{"line":54,"column":226,"offset":2699},"end":{"line":54,"column":246,"offset":2719}}}],"position":{"start":{"line":54,"column":225,"offset":2698},"end":{"line":54,"column":305,"offset":2778}}},{"type":"text","value":".","position":{"start":{"line":54,"column":305,"offset":2778},"end":{"line":54,"column":306,"offset":2779}}}],"position":{"start":{"line":54,"column":1,"offset":2474},"end":{"line":54,"column":306,"offset":2779}}},{"type":"text","value":"\n"},{"type":"element","tagName":"li","properties":{},"children":[{"type":"element","tagName":"strong","properties":{},"children":[{"type":"text","value":"Simplify use of the WebGL2 API","position":{"start":{"line":55,"column":5,"offset":2784},"end":{"line":55,"column":35,"offset":2814}}}],"position":{"start":{"line":55,"column":3,"offset":2782},"end":{"line":55,"column":37,"offset":2816}}},{"type":"text","value":" - Using the raw WebGL API is notoriously verbose and fiddly. luma.gl's classes provide the standard WebGL2 objects and methods, but take care of all the tedious default parameters and object bindings behind the scenes.","position":{"start":{"line":55,"column":37,"offset":2816},"end":{"line":55,"column":256,"offset":3035}}}],"position":{"start":{"line":55,"column":1,"offset":2780},"end":{"line":55,"column":256,"offset":3035}}},{"type":"text","value":"\n"},{"type":"element","tagName":"li","properties":{},"children":[{"type":"element","tagName":"strong","properties":{},"children":[{"type":"text","value":"Shader Programming","position":{"start":{"line":56,"column":5,"offset":3040},"end":{"line":56,"column":23,"offset":3058}}}],"position":{"start":{"line":56,"column":3,"offset":3038},"end":{"line":56,"column":25,"offset":3060}}},{"type":"text","value":" - luma.gl's shadertools is a GLSL module system that provides extensive facilities for developing, modularizing, debugging and profiling GLSL shaders.","position":{"start":{"line":56,"column":25,"offset":3060},"end":{"line":56,"column":176,"offset":3211}}}],"position":{"start":{"line":56,"column":1,"offset":3036},"end":{"line":56,"column":176,"offset":3211}}},{"type":"text","value":"\n"},{"type":"element","tagName":"li","properties":{},"children":[{"type":"element","tagName":"strong","properties":{},"children":[{"type":"text","value":"Performance First","position":{"start":{"line":57,"column":5,"offset":3216},"end":{"line":57,"column":22,"offset":3233}}}],"position":{"start":{"line":57,"column":3,"offset":3214},"end":{"line":57,"column":24,"offset":3235}}},{"type":"text","value":" - luma.gl has strong focus on performance, which includes a preference for providing APIs on lower abstraction levels than some popular WebGL frameworks, and an emphasis of using features such as ","position":{"start":{"line":57,"column":24,"offset":3235},"end":{"line":57,"column":221,"offset":3432}}},{"type":"element","tagName":"em","properties":{},"children":[{"type":"text","value":"instanced rendering","position":{"start":{"line":57,"column":222,"offset":3433},"end":{"line":57,"column":241,"offset":3452}}}],"position":{"start":{"line":57,"column":221,"offset":3432},"end":{"line":57,"column":242,"offset":3453}}},{"type":"text","value":" for large data sets.","position":{"start":{"line":57,"column":242,"offset":3453},"end":{"line":57,"column":263,"offset":3474}}}],"position":{"start":{"line":57,"column":1,"offset":3212},"end":{"line":57,"column":263,"offset":3474}}},{"type":"text","value":"\n"},{"type":"element","tagName":"li","properties":{},"children":[{"type":"element","tagName":"strong","properties":{},"children":[{"type":"text","value":"Doing Computations on the GPU","position":{"start":{"line":58,"column":5,"offset":3479},"end":{"line":58,"column":34,"offset":3508}}}],"position":{"start":{"line":58,"column":3,"offset":3477},"end":{"line":58,"column":36,"offset":3510}}},{"type":"text","value":" - A focus on use cases like GPU based computing using ","position":{"start":{"line":58,"column":36,"offset":3510},"end":{"line":58,"column":91,"offset":3565}}},{"type":"element","tagName":"em","properties":{},"children":[{"type":"text","value":"transform feedback","position":{"start":{"line":58,"column":92,"offset":3566},"end":{"line":58,"column":110,"offset":3584}}}],"position":{"start":{"line":58,"column":91,"offset":3565},"end":{"line":58,"column":111,"offset":3585}}},{"type":"text","value":", and other WebGL2 and ","position":{"start":{"line":58,"column":111,"offset":3585},"end":{"line":58,"column":134,"offset":3608}}},{"type":"element","tagName":"a","properties":{"href":"https://en.wikipedia.org/wiki/General-purpose_computing_on_graphics_processing_units"},"children":[{"type":"text","value":"GPGPU","position":{"start":{"line":58,"column":135,"offset":3609},"end":{"line":58,"column":140,"offset":3614}}}],"position":{"start":{"line":58,"column":134,"offset":3608},"end":{"line":58,"column":227,"offset":3701}}},{"type":"text","value":" techniques.","position":{"start":{"line":58,"column":227,"offset":3701},"end":{"line":58,"column":239,"offset":3713}}}],"position":{"start":{"line":58,"column":1,"offset":3475},"end":{"line":58,"column":239,"offset":3713}}},{"type":"text","value":"\n"}],"position":{"start":{"line":53,"column":1,"offset":2158},"end":{"line":58,"column":239,"offset":3713}}},{"type":"text","value":"\n"},{"type":"element","tagName":"h2","properties":{"id":"history"},"children":[{"type":"element","tagName":"a","properties":{"href":"#history","aria-hidden":true,"class":"anchor"},"children":[{"type":"raw","value":"<svg aria-hidden=\"true\" height=\"16\" version=\"1.1\" viewBox=\"0 0 16 16\" width=\"16\"><path fill-rule=\"evenodd\" d=\"M4 9h1v1H4c-1.5 0-3-1.69-3-3.5S2.55 3 4 3h4c1.45 0 3 1.69 3 3.5 0 1.41-.91 2.72-2 3.25V8.59c.58-.45 1-1.27 1-2.09C10 5.22 8.98 4 8 4H4c-.98 0-2 1.22-2 2.5S3 9 4 9zm9-3h-1v1h1c1 0 2 1.22 2 2.5S13.98 12 13 12H9c-.98 0-2-1.22-2-2.5 0-.83.42-1.64 1-2.09V6.25c-1.09.53-2 1.84-2 3.25C6 11.31 7.55 13 9 13h4c1.45 0 3-1.69 3-3.5S14.5 6 13 6z\"></path></svg>"}]},{"type":"text","value":"History","position":{"start":{"line":61,"column":4,"offset":3719},"end":{"line":61,"column":11,"offset":3726}}}],"position":{"start":{"line":61,"column":1,"offset":3716},"end":{"line":61,"column":11,"offset":3726}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"luma.gl was originally created in late 2015 as a fork of ","position":{"start":{"line":63,"column":1,"offset":3728},"end":{"line":63,"column":58,"offset":3785}}},{"type":"element","tagName":"a","properties":{"href":"https://github.com/philogb/philogl"},"children":[{"type":"text","value":"PhiloGL","position":{"start":{"line":63,"column":59,"offset":3786},"end":{"line":63,"column":66,"offset":3793}}}],"position":{"start":{"line":63,"column":58,"offset":3785},"end":{"line":63,"column":103,"offset":3830}}},{"type":"text","value":" to provide high performance WebGL rendering capability for ","position":{"start":{"line":63,"column":103,"offset":3830},"end":{"line":63,"column":163,"offset":3890}}},{"type":"element","tagName":"a","properties":{"href":"https://github.com/uber/deck.gl"},"children":[{"type":"text","value":"deck.gl","position":{"start":{"line":63,"column":164,"offset":3891},"end":{"line":63,"column":171,"offset":3898}}}],"position":{"start":{"line":63,"column":163,"offset":3890},"end":{"line":63,"column":205,"offset":3932}}},{"type":"text","value":" - a 3D visualization framework for large scale data.","position":{"start":{"line":63,"column":205,"offset":3932},"end":{"line":63,"column":258,"offset":3985}}}],"position":{"start":{"line":63,"column":1,"offset":3728},"end":{"line":63,"column":258,"offset":3985}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"With the increased adoption of the deck.gl framework, usage of luma.gl has also gradually increased. In addition, various contributors have started to create up their own custom deck.gl layers for their apps, which requires usage of luma.gl's classes and APIs. This triggered a major rewrite the documentation and the website.","position":{"start":{"line":65,"column":1,"offset":3987},"end":{"line":65,"column":327,"offset":4313}}}],"position":{"start":{"line":65,"column":1,"offset":3987},"end":{"line":65,"column":327,"offset":4313}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"The arrival of WebGL2 was a major milestone in the WebGL landscape. With the release of luma.gl v4 in July 2017, luma.gl was positioned as a foundation library for high-performance GPU programming in JavaScript, and luma.gl v5 and v6 series releases have continued to provide incremental improvements in the WebGL2 and GPGPU areas.","position":{"start":{"line":67,"column":1,"offset":4315},"end":{"line":67,"column":332,"offset":4646}}}],"position":{"start":{"line":67,"column":1,"offset":4315},"end":{"line":67,"column":332,"offset":4646}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"Today, luma.gl is the foundational WebGL library in the vis.gl framework suite, but is designed so that it can be used stand-alone.","position":{"start":{"line":69,"column":1,"offset":4648},"end":{"line":69,"column":132,"offset":4779}}}],"position":{"start":{"line":69,"column":1,"offset":4648},"end":{"line":69,"column":132,"offset":4779}}},{"type":"text","value":"\n"},{"type":"element","tagName":"h2","properties":{"id":"comparison-with-other-webgl-frameworks"},"children":[{"type":"element","tagName":"a","properties":{"href":"#comparison-with-other-webgl-frameworks","aria-hidden":true,"class":"anchor"},"children":[{"type":"raw","value":"<svg aria-hidden=\"true\" height=\"16\" version=\"1.1\" viewBox=\"0 0 16 16\" width=\"16\"><path fill-rule=\"evenodd\" d=\"M4 9h1v1H4c-1.5 0-3-1.69-3-3.5S2.55 3 4 3h4c1.45 0 3 1.69 3 3.5 0 1.41-.91 2.72-2 3.25V8.59c.58-.45 1-1.27 1-2.09C10 5.22 8.98 4 8 4H4c-.98 0-2 1.22-2 2.5S3 9 4 9zm9-3h-1v1h1c1 0 2 1.22 2 2.5S13.98 12 13 12H9c-.98 0-2-1.22-2-2.5 0-.83.42-1.64 1-2.09V6.25c-1.09.53-2 1.84-2 3.25C6 11.31 7.55 13 9 13h4c1.45 0 3-1.69 3-3.5S14.5 6 13 6z\"></path></svg>"}]},{"type":"text","value":"Comparison with other WebGL frameworks","position":{"start":{"line":72,"column":4,"offset":4785},"end":{"line":72,"column":42,"offset":4823}}}],"position":{"start":{"line":72,"column":1,"offset":4782},"end":{"line":72,"column":42,"offset":4823}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"luma.gl is the natural choice if you are working with any of the WebGL-based frameworks in the vis.gl suite (deck.gl, kepler.gl etc).","position":{"start":{"line":74,"column":1,"offset":4825},"end":{"line":74,"column":134,"offset":4958}}}],"position":{"start":{"line":74,"column":1,"offset":4825},"end":{"line":74,"column":134,"offset":4958}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"If not, and you are considering what WebGL framework to use in an independent project, then as a first step towards a decision we recommend that you take a look at the things that have been built on luma.gl (e.g. vis.gl), make sure the luma.gl design philosophy resonates with you, and finally, review the luma.gl roadmap to see that the library is heading in a direction that is meaningful to you.","position":{"start":{"line":76,"column":1,"offset":4960},"end":{"line":76,"column":399,"offset":5358}}}],"position":{"start":{"line":76,"column":1,"offset":4960},"end":{"line":76,"column":399,"offset":5358}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"We think that luma.gl could be a great choice if:","position":{"start":{"line":78,"column":1,"offset":5360},"end":{"line":78,"column":50,"offset":5409}}}],"position":{"start":{"line":78,"column":1,"offset":5360},"end":{"line":78,"column":50,"offset":5409}}},{"type":"text","value":"\n"},{"type":"element","tagName":"ul","properties":{},"children":[{"type":"text","value":"\n"},{"type":"element","tagName":"li","properties":{},"children":[{"type":"text","value":"you want to work with, learn, and leverage the power of the WebGL2 API.","position":{"start":{"line":80,"column":3,"offset":5413},"end":{"line":80,"column":74,"offset":5484}}}],"position":{"start":{"line":80,"column":1,"offset":5411},"end":{"line":80,"column":74,"offset":5484}}},{"type":"text","value":"\n"},{"type":"element","tagName":"li","properties":{},"children":[{"type":"text","value":"you want \"more control\" by having the option of working close to WebGL.","position":{"start":{"line":81,"column":3,"offset":5487},"end":{"line":81,"column":74,"offset":5558}}}],"position":{"start":{"line":81,"column":1,"offset":5485},"end":{"line":81,"column":74,"offset":5558}}},{"type":"text","value":"\n"},{"type":"element","tagName":"li","properties":{},"children":[{"type":"text","value":"you want to do shader coding","position":{"start":{"line":82,"column":3,"offset":5561},"end":{"line":82,"column":31,"offset":5589}}}],"position":{"start":{"line":82,"column":1,"offset":5559},"end":{"line":82,"column":31,"offset":5589}}},{"type":"text","value":"\n"},{"type":"element","tagName":"li","properties":{},"children":[{"type":"text","value":"you are focusing on rendering large data sets with high performance.","position":{"start":{"line":83,"column":3,"offset":5592},"end":{"line":83,"column":71,"offset":5660}}}],"position":{"start":{"line":83,"column":1,"offset":5590},"end":{"line":83,"column":71,"offset":5660}}},{"type":"text","value":"\n"}],"position":{"start":{"line":80,"column":1,"offset":5411},"end":{"line":83,"column":71,"offset":5660}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"We feel that luma.gl is currently not the strongest choice if:","position":{"start":{"line":85,"column":1,"offset":5662},"end":{"line":85,"column":63,"offset":5724}}}],"position":{"start":{"line":85,"column":1,"offset":5662},"end":{"line":85,"column":63,"offset":5724}}},{"type":"text","value":"\n"},{"type":"element","tagName":"ul","properties":{},"children":[{"type":"text","value":"\n"},{"type":"element","tagName":"li","properties":{},"children":[{"type":"text","value":"You wish to avoid learning anything about WebGL, working exclusively with higher abstractions.","position":{"start":{"line":87,"column":3,"offset":5728},"end":{"line":87,"column":97,"offset":5822}}}],"position":{"start":{"line":87,"column":1,"offset":5726},"end":{"line":87,"column":97,"offset":5822}}},{"type":"text","value":"\n"},{"type":"element","tagName":"li","properties":{},"children":[{"type":"text","value":"You need to load 3D models from various formats.","position":{"start":{"line":88,"column":3,"offset":5825},"end":{"line":88,